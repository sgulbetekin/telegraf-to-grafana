          "queryType": "raw",
          "rawQuery": "SELECT BIN(time, 1m) AS time_bin,\n    AVG(CASE WHEN measure_name = 'usage_user' THEN measure_value::double ELSE NULL END) AS avg_cpu_user,\n    AVG(CASE WHEN measure_name = 'usage_system' THEN measure_value::double ELSE NULL END) AS avg_cpu_system,\n    AVG(CASE WHEN measure_name = 'usage_idle' THEN measure_value::double ELSE NULL END) AS avg_cpu_idle,\n    AVG(CASE WHEN measure_name = 'usage_iowait' THEN measure_value::double ELSE NULL END) AS avg_cpu_iowait,\n    AVG(CASE WHEN measure_name = 'usage_steal' THEN measure_value::double ELSE NULL END) AS avg_cpu_steal,\n    AVG(CASE WHEN measure_name = 'usage_nice' THEN measure_value::double ELSE NULL END) AS avg_cpu_nice,\n    AVG(CASE WHEN measure_name = 'usage_guest' THEN measure_value::double ELSE NULL END) AS avg_cpu_guest,\n    AVG(CASE WHEN measure_name = 'usage_irq' THEN measure_value::double ELSE NULL END) AS avg_cpu_irq\nFROM ${dbName}.${tableCPU}\nWHERE measure_name IN (\n        'usage_user', 'usage_system', 'usage_idle', 'usage_iowait',\n        'usage_steal', 'usage_nice', 'usage_guest', 'usage_irq'\n    )\n    AND $__timeFilter\nGROUP BY BIN(time, 1m)\nORDER BY time_bin desc",
